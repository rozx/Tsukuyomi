{
  "name": "tsukuyomi-translator",
  "version": "0.9.0.5",
  "description": "Tsukuyomi - Moonlit Translator",
  "productName": "Tsukuyomi - Moonlit Translator",
  "author": "rozx <rozx@heavyskymobile.com>",
  "type": "module",
  "private": true,
  "scripts": {
    "lint": "bunx eslint --quiet .",
    "type-check": "bunx vue-tsc --noEmit",
    "format": "prettier --write \"**/*.{js,ts,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "test": "bun test",
    "bump": "bun scripts/bump-version.ts",
    "setup:git-hooks": "node scripts/setup-git-hooks.js",
    "dev": "quasar dev && bun --watch run server/app-server.ts",
    "dev:vite": "quasar dev",
    "dev:server": "bun --watch run server/app-server.ts",
    "dev:electron": "quasar dev -m electron",
    "build": "quasar build",
    "build:spa": "cross-env NODE_ENV=production quasar build -m spa",
    "build:electron": "cross-env NODE_ENV=production quasar build -m electron",
    "build:electron:debug": "ELECTRON_DEBUG=true quasar build -m electron",
    "postinstall": "quasar prepare",
    "start:proxy": "bun run server/proxy-server.ts",
    "start:app": "bun run server/app-server.ts",
    "start": "bun run start:app",
    "build:deploy": "bash scripts/build.sh",
    "start:deploy": "bash scripts/run.sh"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@octokit/rest": "^22.0.1",
    "@primevue/themes": "^4.5.0",
    "@quasar/extras": "^1.17.0",
    "axios": "^1.13.2",
    "cheerio": "^1.1.2",
    "co": "^4.6.0",
    "dompurify": "^3.3.0",
    "express": "^4.22.1",
    "fuse.js": "^7.0.0",
    "http-proxy-middleware": "^3.0.5",
    "idb": "^8.0.3",
    "lodash": "^4.17.21",
    "marked": "^17.0.1",
    "openai": "^6.9.1",
    "pinia": "^3.0.4",
    "primeicons": "^7.0.0",
    "primevue": "^4.5.0",
    "puppeteer": "^24.31.0",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "puppeteer-in-electron": "^3.0.5",
    "quasar": "^2.18.6",
    "uuid": "^13.0.0",
    "vue": "^3.5.25",
    "vue-i18n": "^11.2.2",
    "vue-router": "^4.6.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@intlify/unplugin-vue-i18n": "^4.0.0",
    "@quasar/app-vite": "^2.4.0",
    "@types/bun": "^1.3.3",
    "@types/co": "^4.6.6",
    "@types/dompurify": "^3.2.0",
    "@types/electron": "^1.6.12",
    "@types/express": "^4.17.25",
    "@types/lodash": "^4.17.21",
    "@types/marked": "^6.0.0",
    "@types/node": "^24.10.1",
    "@types/uuid": "^11.0.0",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "autoprefixer": "^10.4.22",
    "cross-env": "^10.1.0",
    "electron": "^39.2.4",
    "electron-builder": "^26.0.12",
    "eslint": "^9.39.1",
    "eslint-plugin-vue": "^10.6.2",
    "fake-indexeddb": "^6.2.5",
    "globals": "^16.5.0",
    "prettier": "^3.7.3",
    "tailwindcss": "^3.4.18",
    "typescript": "^5.9.3",
    "unplugin-auto-import": "^20.3.0",
    "unplugin-vue-components": "^30.0.0",
    "vite-plugin-checker": "^0.11.0",
    "vite-plugin-node-polyfills": "^0.24.0",
    "vue-eslint-parser": "^10.2.0",
    "vue-tsc": "^3.1.5"
  },
  "engines": {
    "bun": ">=1.0.0"
  }
}
