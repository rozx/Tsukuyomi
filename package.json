{
  "name": "luna-ai-translator",
  "version": "0.5.0",
  "description": "Luna AI translator",
  "productName": "Luna AI translator",
  "author": "rozx <rozx@heavyskymobile.com>",
  "type": "module",
  "private": true,
  "scripts": {
    "lint": "bunx eslint",
    "type-check": "bunx vue-tsc --noEmit",
    "format": "prettier --write \"**/*.{js,ts,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "test": "bun test",
    "sync-version": "bun scripts/sync-version.ts",
    "dev": "quasar dev && bun --watch run server/app-server.ts",
    "dev:vite": "quasar dev",
    "dev:server": "bun --watch run server/app-server.ts",
    "dev:electron": "quasar dev -m electron",
    "build": "bun run sync-version && quasar build",
    "build:spa": "bun run sync-version && quasar build -m spa",
    "build:electron": "bun run sync-version && quasar build -m electron",
    "build:electron:debug": "ELECTRON_DEBUG=true bun run sync-version && quasar build -m electron",
    "postinstall": "quasar prepare",
    "start:proxy": "bun run server/proxy-server.ts",
    "start:app": "bun run server/app-server.ts",
    "start": "bun run start:app",
    "build:deploy": "bash scripts/build.sh",
    "start:deploy": "bash scripts/run.sh"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@octokit/rest": "^22.0.1",
    "@patdx/kuromoji": "^1.0.4",
    "@primevue/themes": "^4",
    "@quasar/extras": "^1.16.4",
    "axios": "^1.2.1",
    "cheerio": "^1.1.2",
    "cors": "^2.8.5",
    "dompurify": "^3.3.0",
    "express": "^4.18.2",
    "http-proxy-middleware": "^3.0.5",
    "idb": "^8.0.3",
    "impit": "^0.7.1",
    "lodash": "^4.17.21",
    "marked": "^17.0.1",
    "openai": "^6.9.0",
    "pinia": "^3.0.1",
    "primeicons": "^7",
    "primevue": "^4",
    "quasar": "^2.16.0",
    "uuid": "^13.0.0",
    "vue": "^3.5.22",
    "vue-i18n": "^11.0.0",
    "vue-router": "^4.0.12"
  },
  "devDependencies": {
    "@electron/packager": "^18.3.2",
    "@eslint/js": "^9.14.0",
    "@intlify/unplugin-vue-i18n": "^4.0.0",
    "@quasar/app-vite": "^2.1.0",
    "@types/bun": "^1.3.3",
    "@types/cors": "^2.8.17",
    "@types/dompurify": "^3.2.0",
    "@types/electron": "^1.6.12",
    "@types/express": "^4.17.21",
    "@types/lodash": "^4.17.20",
    "@types/marked": "^6.0.0",
    "@types/node": "^24.10.1",
    "@types/uuid": "^11.0.0",
    "@vue/eslint-config-prettier": "^10.1.0",
    "@vue/eslint-config-typescript": "^14.4.0",
    "autoprefixer": "^10.4.2",
    "electron": "^39.2.1",
    "eslint": "^9.14.0",
    "eslint-plugin-vue": "^10.4.0",
    "globals": "^16.4.0",
    "prettier": "^3.3.3",
    "tailwindcss": "^3",
    "typescript": "^5.9.2",
    "unplugin-auto-import": "^20.2.0",
    "unplugin-vue-components": "^30.0.0",
    "vite-plugin-checker": "^0.11.0",
    "vite-plugin-node-polyfills": "^0.24.0",
    "vue-eslint-parser": "^10.2.0",
    "vue-tsc": "^3.0.7"
  },
  "engines": {
    "bun": ">=1.0.0"
  }
}
